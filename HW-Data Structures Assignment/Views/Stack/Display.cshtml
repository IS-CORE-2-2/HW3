
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Display Stack</title>
</head>
<body>
    <div>
        @foreach (var sData in ViewBag.MyStack)
        {
            <p>@sData</p>
        }
        <a href="@Url.Action("Index", "Stack")"
           class="w3-bar-item w3-button">Return to Stack Menu</a>
    </div>
</body>
</html>
